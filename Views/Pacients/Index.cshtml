@model IEnumerable<API.Pacient>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.F)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.I)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.O)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Born)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PasportSeria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PasportAdres)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rabota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdMedCardNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.F)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.I)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.O)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Born)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasportSeria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasportAdres)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rabota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdMedCardNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
